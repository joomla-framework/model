{
    "name": "joomla/model",
    "type": "joomla-package",
    "description": "Joomla Model Package",
    "keywords": ["joomla", "framework", "model"],
    "homepage": "https://github.com/joomla-framework/model",
    "license": "GPL-2.0+",
    "require": {
        "php": "~7.0"
    },
    "require-dev": {
        "joomla/test": "~1.0",
        "joomla/database": "~1.0|~2.0",
        "joomla/registry": "^1.4.5|~2.0",
        "phpunit/phpunit": "~6.0",
        "squizlabs/php_codesniffer": "1.*"
    },
    "suggest": {
        "joomla/database": "Allows using database models",
        "joomla/registry": "Allows using stateful models"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Model\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Model\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}
