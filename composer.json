{
    "name": "joomla/model",
    "type": "joomla-package",
    "description": "Joomla Model Package",
    "keywords": ["joomla", "framework", "model"],
    "homepage": "https://github.com/joomla-framework/model",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^7.2.5"
    },
    "require-dev": {
        "joomla/coding-standards": "^3.0@dev",
        "joomla/database": "^1.0|^2.0",
        "joomla/registry": "^1.4.5|^2.0",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "suggest": {
        "joomla/database": "^1.0|^2.0 Allows using database models",
        "joomla/registry": "^1.4.5|^2.0 Allows using stateful models"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Model\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Model\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}
